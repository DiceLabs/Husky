<?xml version="1.0"?>
<launch>
  <arg name="camera_depth_optical_frame"
     default='camera_realsense_link_gazebo'/>
  <arg name="map_frame" default='map'/>
  <node pkg="ml_test" type="robot_navigation_to_object.py" name="robot_navigation_to_object" output="screen">
  <rosparam file="$(find ml_test)/config/robot_navigation_to_object.yaml" />   
     <param name="camera_depth_optical_frame"
     value="$(arg camera_depth_optical_frame)"/>
     <param name="map_frame" value="$(arg mP_frame)"/>
  </node>
 
</launch> 
